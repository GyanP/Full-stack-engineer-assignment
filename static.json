{
  "root": "client/build/",
  "clean_urls": false,
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=31557600",
      "X-Frame-Options": "SAMEORIGIN",
      "X-Content-Type-Options": "nosniff",
      "X-XSS-Protection": "1; mode=block"
    },
    "/": {
      "Cache-Control": "no-store"
    }
  }
}
